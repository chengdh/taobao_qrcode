= form_for @logo do |f|
  - if @logo.errors.any?
    #error_explanation
      %h2= "#{pluralize(@logo.errors.count, "error")} prohibited this logo from being saved:"
      %ul
        - @logo.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nick
    = f.text_field :nick
  .actions
    = f.submit 'Save'
